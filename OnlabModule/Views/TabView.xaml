<UserControl x:Class="OnlabModule.Views.TabView"
             xmlns:prism="http://www.codeplex.com/prism"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OnlabModule.Views"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="Csapatok">
                <ItemsControl Name="AddTeamTabRegion" prism:RegionManager.RegionName="AddTeamTabRegion"/>
            </TabItem>
            <TabItem Header="Pontok">
                <ItemsControl Name="SummaryRegion" prism:RegionManager.RegionName="SummaryRegion"/>
            </TabItem>
            <TabItem Header="Időmérés">
                <ItemsControl Name="TimeMeasureRegion" prism:RegionManager.RegionName="TimeMeasureRegion"/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
